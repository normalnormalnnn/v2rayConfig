{
    "inbounds":[
        {
            "port":"port",
            "protocol":"vmess",
            "settings":{
                "clients":[
                    {
                        "id":"id",
                        "alterId":1
                    }
                ]
            },
            "sniffing":{
                "enabled":true,
                "destOverride":[
                    "http",
                    "tls"
                ]
            },
            "streamSettings":{
                "network":"h2",
                "httpSettings":{
                    "path":"/test"
                },
                "security":"tls",
                "tlsSettings":{
                    "certificates":[
                        {
                            "certificateFile":"/etc/v2ray/v2ray.pem",
                            "keyFile":"/etc/v2ray/v2ray.key"
                        }
                    ]
                }
            }
        }
    ],
    "outbounds":[
        {
            "protocol":"freedom",
            "settings":{

            },
            "tag":"direct"
        },
        {
            "protocol":"socks",
            "settings":{
                "servers":[
                    {
                        "address":"127.0.0.1",
                        "port":"port"
                    }
                ]
            },
            "tag":"china"
        }
    ],
    "routing":{
        "domainStrategy":"IPIfNotMatch",
        "rules":[
            {
                "type":"field",
                "outboundTag":"china",
                "domain":[
                    "geosite:cn"
                ]
            },
            {
                "type":"field",
                "outboundTag":"china",
                "ip":[
                    "geoip:cn",
                    "geoip:private"
                ]
            }
        ]
    }
}