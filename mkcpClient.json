{
    "log":{
        "loglevel":"debug",
        "access":"/var/log/v2ray/access.log",
        "error":"/var/log/v2ray/error.log"
    },
    "inbounds":[
        {
            "port":1080,
            "listen":"127.0.0.1",
            "protocol":"socks",
            "settings":{
                "auth":"noauth",
                "udp":false
            }
        }
    ],
    "outbounds":[
        {
            "protocol":"vmess",
            "settings":{
                "vnext":[
                    {
                        "address":"ip",
                        "port":"port",
                        "users":[
                            {
                                "id":"id",
                                "alterId":1
                            }
                        ]
                    }
                ]
            },
            "streamSettings":{
                "network":"mkcp",
                "kcpSettings":{
                    "mtu":1350,
                    "tti":20,
                    "uplinkCapacity":2,
                    "downlinkCapacity":500,
                    "congestion":true,
                    "readBufferSize":100,
                    "writeBufferSize":100,
                    "header":{
                        "type":"none"
                    }
                }
            }
        }
    ]
}