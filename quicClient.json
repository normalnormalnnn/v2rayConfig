{
    "log":{
        "loglevel":"debug",
        "access":"/var/log/v2ray/access.log",
        "error":"/var/log/v2ray/error.log"
    },
    "inbounds":[
        {
            "port":1080,
            "listen":"127.0.0.1",
            "protocol":"socks",
            "settings":{
                "auth":"noauth",
                "udp":false
            }
        }
    ],
    "outbounds":[
        {
            "protocol":"vmess",
            "settings":{
                "vnext":[
                    {
                        "address":"ip",
                        "port":"port",
                        "users":[
                            {
                                "id":"id",
                                "alterId":1
                            }
                        ]
                    }
                ]
            },
            "streamSettings":{
                "network":"quic",
                "quickSettings":{
                    "security":"none",
                    "key":"",
                    "header":{
                        "type":"none"
                    }
                }
            }
        }
    ]
}